# GitHub credentials for accessing private repositories/packages
# GITHUB_TOKEN: Personal Access Token with 'read:packages' permission
set -x GITHUB_TOKEN "your_github_token_here"
set -x GITHUB_ACTOR "your_github_username_here"

# Spotify API credentials for your music app
# SPOTIFY_CLIENT_ID: Your Spotify app's Client ID from Spotify Developer Dashboard
set -x SPOTIFY_CLIENT_ID "your_spotify_client_id_here"
# SPOTIFY_CLIENT_SECRET: Your Spotify app's Client Secret (if needed for server-side auth)
set -x SPOTIFY_CLIENT_SECRET "your_spotify_client_secret_here"
